{
  "name": "linter-lsc",
  "main": "./src/index",
  "version": "2.4.0",
  "description": "Atom linter plugin for LiveScript, using lsc.",
  "repository": "https://github.com/AtomLinter/linter-lsc.git",
  "license": "MIT",
  "scripts": {
    "test": "apm test",
    "lint": "eslint ."
  },
  "engines": {
    "atom": ">=1.7.0 <2.0.0"
  },
  "keywords": [
    "atom",
    "livescript",
    "linter",
    "lsc"
  ],
  "package-deps": [
    "linter:2.0.0"
  ],
  "dependencies": {
    "atom-linter": "^10.0.0",
    "atom-package-deps": "^5.0.0",
    "livescript": "1.5.0"
  },
  "devDependencies": {
    "eslint": "^5.0.0",
    "eslint-config-airbnb-base": "^13.0.0",
    "eslint-plugin-import": "^2.7.0",
    "jasmine-fix": "^1.0.1"
  },
  "providedServices": {
    "linter": {
      "versions": {
        "2.0.0": "provideLinter"
      }
    }
  },
  "eslintConfig": {
    "extends": "airbnb-base",
    "rules": {
      "global-require": "off",
      "import/no-unresolved": [
        "error",
        {
          "ignore": [
            "atom"
          ]
        }
      ]
    },
    "globals": {
      "atom": true
    },
    "env": {
      "node": true,
      "browser": true
    }
  }
}
